07455],\n                {&quot;bubblingMouseEvents&quot;: true, &quot;color&quot;: &quot;#000000&quot;, &quot;dashArray&quot;: null, &quot;dashOffset&quot;: null, &quot;fill&quot;: true, &quot;fillColor&quot;: &quot;#000000&quot;, &quot;fillOpacity&quot;: 0.2, &quot;fillRule&quot;: &quot;evenodd&quot;, &quot;lineCap&quot;: &quot;round&quot;, &quot;lineJoin&quot;: &quot;round&quot;, &quot;opacity&quot;: 1.0, &quot;radius&quot;: 1000, &quot;stroke&quot;: true, &quot;weight&quot;: 3}\n            ).addTo(map_eff4048d282ede840e0540e2f9938ced);\n        \n    \n        var popup_6acc7692db073eb6b471348698943b89 = L.popup({&quot;maxWidth&quot;: &quot;100%&quot;});\n\n        \n            \n                var html_b08b5212ecfaf427408b0bb81c84734a = $(`&lt;div id=&quot;html_b08b5212ecfaf427408b0bb81c84734a&quot; style=&quot;width: 100.0%; height: 100.0%;&quot;&gt;VAFB SLC-4E&lt;/div&gt;`)[0];\n                popup_6acc7692db073eb6b471348698943b89.setContent(html_b08b5212ecfaf427408b0bb81c84734a);\n            \n        \n\n        circle_04d07302b724dc8a6d115ba0ab4e1566.bindPopup(popup_6acc7692db073eb6b471348698943b89)\n        ;\n\n        \n    \n    \n            var marker_03bf6016594e8dc171d7d84329743146 = L.marker(\n                [34.63283416, -120.6107455],\n                {}\n            ).addTo(map_eff4048d282ede840e0540e2f9938ced);\n        \n    \n            var div_icon_d86672d9bd9fdbd6ac6181a29ff5dbaa = L.divIcon({&quot;className&quot;: &quot;empty&quot;, &quot;html&quot;: &quot;\\u003cdiv style=\\&quot;font-size: 12; color:#d35400;\\&quot;\\u003e\\u003cb\\u003eVAFB SLC-4E\\u003c/b\\u003e\\u003c/div\\u003e&quot;, &quot;iconAnchor&quot;: [0, 0], &quot;iconSize&quot;: [20, 20]});\n            marker_03bf6016594e8dc171d7d84329743146.setIcon(div_icon_d86672d9bd9fdbd6ac6181a29ff5dbaa);\n        \n&lt;/script&gt;\n&lt;/html&gt;\" style=\"position:absolute;width:100%;height:100%;left:0;top:0;border:none !important;\" allowfullscreen webkitallowfullscreen mozal